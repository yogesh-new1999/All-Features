<mvc:View xmlns:table="sap.ui.table"
    controllerName="showshopping.controller.master"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
<App>
	
			<Page title="{i18n>ordersListCategories}">
				<List id="orders">
				<items>
                    <StandardListItem title="{i18n>categories}" press=".categoriesDetailView" type="Navigation"/>
                </items>
                <items>
                    <StandardListItem title="{i18n>Products}" press=".onProducts" type="Navigation"/>
                
                </items>
                <items>
                    <StandardListItem title="{i18n>Shippers}" press=".onShippers" type="Navigation"/>
                </items>
				</List>
			</Page>
</App>

</mvc:View>